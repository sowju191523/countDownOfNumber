import java.util.*;
class hello
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       countDown(n);
    }
    static void countDown(int n)
    {
        if(n==-1)
        {
            return;
        }
        System.out.println(n);
        countDown(n-1);
    }
}
